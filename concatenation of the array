import java.util.*;

public class Main {
    public static void main(String[] args) {
      int arr[]={1,2,3};
      int n=arr.length;
      
      int result[]=new int[2*n];
      for(int i=0;i<n;i++){
        result[i]=arr[i];
        result[i+n]=arr[i];
      }
      System.out.print("the final array is:"+Arrays.toString(result));
  }
}
