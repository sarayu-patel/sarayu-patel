import java.util.*;

public class Main {
    public static void main(String[] args) {
      int n=155;
      int temp=n;
      int sum=0;
      int r;
      while(n>0){
        r=n%10;
        n=n/10;
        sum=sum+ r*r*r;
      }
      if(temp==sum){
        System.out.println("it is armstrong number");
        
      }
      else{
        System.out.println("it is not an armstrong number");
      }
  }
}
