import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		int rev=0;
		int rem=0;
		Scanner sc= new Scanner(System.in);
		System.out.println("enter a number");
		int n=sc.nextInt();
		int original=n;
		while(n!=0){
		    rem=n%10;
		    rev=rev*10+rem;
		    n/=10;
		    
		}
		if(original==rev){
		    System.out.println("it is palindrome");
		}
		else{
		    System.out.println("it is not palindrome");
		}
	}
}
