
public class singleNumber {
    public static void main(String[] args) {
        int arr[]={1,1,4,4,7,3,7};
        System.out.println(ans(arr));
    }
    private static int ans(int arr[]){
        int unique=0;
        for(int n:arr){
            unique^=n;
        }
        return unique;
    }
    
}
